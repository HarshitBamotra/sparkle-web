option_settings:
    aws:elasticbeanstalk:container:python:
        WSGIPath: Eshop.wsgi:application

    aws:elasticbeanstalk:environment:proxy:staticfiles:
        /static: static

container_commands:
  01_migrate:
    command: "source /opt/python/run/venv/bin/activate && python main_platform/manage.py migrate --noinput"
    leader_only: true
  02_createsu:
      command: "source /opt/python/run/venv/bin/activate && python main_platform/manage.py createsu"
      leader_only: true
  03_collectstatic:
    command: "source /opt/python/run/venv/bin/activate && python main_platform/manage.py collectstatic --noinput"