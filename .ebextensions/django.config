option_settings:
    aws:elasticbeanstalk:container:python:
        WSGIPath: Eshop/wsgi.py

    aws:elasticbeanstalk:environment:proxy:staticfiles:
        /static: static

container_commands:
    01_collectstatic:
        command: "source /var/app/venv/*/bin/activate && python3 manage.py migrate --noinput --clear"
    02_migrate:
        command: "source /var/app/venv/*/bin/activate && python3 manage.py migrate --noinput"
        leader_only: true